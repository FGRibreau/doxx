// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.js.map": true,
    "**/*.js": {
      "when": "$(basename).ts"
    }
  },

  "antlr4.generation": {
    "mode": "internal",
    "language": "TypeScript",
    "listeners": true,
    "visitors": false
  }
}
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "moca",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "cwd": "${workspaceRoot}",
      "args": [
        "test/*.js",
        "--no-timeouts"
      ],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "moca: scanner",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "cwd": "${workspaceRoot}",
      "args": [
        "test/scanner.js",
        "--no-timeouts"
      ],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "moca: parser",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "cwd": "${workspaceRoot}",
      "args": [
        "test/parser.js",
        "--no-timeouts"
      ],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Scanner",
      "program": "${workspaceRoot}/test/scanner.ts",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Parser",
      "program": "${workspaceRoot}/test/parser.ts",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Main",
      "program": "${workspaceRoot}/main.js",
      "cwd": "${workspaceRoot}",
      "sourceMaps": true
    },
  ]
}